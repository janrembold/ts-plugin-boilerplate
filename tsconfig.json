{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"declaration": false,
		"module": "CommonJS",
		"moduleResolution": "Node",
		"noImplicitAny": true,
		"removeComments": true,
		"suppressImplicitAnyIndexErrors": true,
		"target": "ES5",
		"lib": [
			"DOM",
			"ES2015",
			"ES5"
		]
	},
	"typeRoots": [
		"node_modules/@types"
	],
	"types": [
		"jquery",
		"webpack",
		"webpack-env"
	],
	"exclude": [
		"dist",
		"example",
		"node_modules",
		"test"
	]
}
